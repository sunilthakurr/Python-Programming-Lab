# Task 4 numpy questions python
import numpy as np
print("Enter the row and column of matrix")
r,c = list(map(int,input().split()))

M1 = []
M2 = []

for i in range (r):
    ele = list(map(int,input().split()))
    M1.append(ele)

for i in range (r):
    ele = list(map(int,input().split()))
    M2.append(ele)


M1 = np.array(M1)
M2 = np.array(M2)

print("Matrix after addition")
print(np.add(M1,M2))
print("Matrix after subtraction")
print(np.subtract(M1,M2))
print("Matrix after Multiplication")
print(np.multiply(M1,M2))
print("Matrix after division")
print(np.floor_divide(M1,M2))
print("Matrix after powering element")
print(np.power(M1,M2))
